{
    "html.customData": [
        "./node_modules/@shoelace-style/shoelace/dist/vscode.html-custom-data.json"
    ],
    "deno.enable": true,
    "files.associations": {
        "**/tailwind*/**/*.css": "tailwindcss",
        "**/templates/**/*.html": "gotmpl"
    },
    "gopls": {
        "ui.semanticTokens": true
    },
    "[go]": {
        "editor.semanticHighlighting.enabled": true
    }
}
